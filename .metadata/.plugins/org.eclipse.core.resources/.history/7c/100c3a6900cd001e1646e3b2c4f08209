package exrcicio01;

public class Main {

	public static void main(String[] args) {
		
		int a = 10;
		int b = 30;
		int soma = a + b;
		
		System.out.println("SOMA = " + soma);
		
		int c = -30;
		int d = 10;
		int somando = c + d; 
		
		System.out.println("SOMA = " + somando);
		
		int e = 0;
		System.out.println("SOMA = " + e);

	}

}
